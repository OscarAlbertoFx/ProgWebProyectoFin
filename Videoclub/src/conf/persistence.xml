<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="VideoclubPU" transaction-type="JTA">
    <jta-data-source>java:app/videoclub</jta-data-source>
    <class>entidad.Categoria</class>
    <class>entidad.Comprapelicula</class>
    <class>entidad.Compraserie</class>
    <class>entidad.Datospagopelicula</class>
    <class>entidad.Datospagoserie</class>
    <class>entidad.Detallecomprapelicula</class>
    <class>entidad.Detallecompraserie</class>
    <class>entidad.Pelicula</class>
    <class>entidad.Serie</class>
    <class>entidad.Usuario</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties/>
  </persistence-unit>
</persistence>
